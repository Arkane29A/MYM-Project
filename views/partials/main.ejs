  <script>
    function logout() {
      fetch('/logout', {
        method: 'GET'
      })
      .then(response => {
        if (response.ok) {
          // Redirect to homepage after successful logout
          window.location.href = '/';
        } else {
          // Handle error response
        }
      })
      .catch(error => {
        // Handle network error
      });
    }
  </script>

